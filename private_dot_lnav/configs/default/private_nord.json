{
    "$schema": "https://lnav.org/schemas/config-v1.schema.json",
    "ui": {
        "theme-defs": {
            "nord": {
                "vars": {
                    "black": "#2e3440",
                    "red": "#bf616a",
                    "green": "#a3be8c",
                    "yellow": "#ebcb8b",
                    "blue": "#5e81ac",
                    "magenta": "#b48ead",
                    "cyan": "#88c0d0",
                    "white": "#eceff4",
                    "orange": "#d08770",
                    "purple": "#b48ead",
                    "pink": "#b48ead",
                    "semantic_highlight_color": "semantic()"
                },
                "styles": {
                    "identifier": {
                        "color": "semantic()"
                    },
                    "text": {
                        "color": "#d8dee9",
                        "background-color": "$black"
                    },
                    "selected-text": {
                        "background-color": "$cyan"
                    },
                    "alt-text": {
                        "background-color": "#2e3440"
                    },
                    "ok": {
                        "color": "$green",
                        "bold": true
                    },
                    "info": {
                        "color": "$magenta",
                        "bold": true
                    },
                    "error": {
                        "color": "$red",
                        "bold": true
                    },
                    "warning": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "hidden": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "cursor-line": {
                        "color": "$cyan",
                        "background-color": "#3b4252",
                        "bold": true
                    },
                    "disabled-cursor-line": {
                        "color": "$cyan",
                        "background-color": "#2e3440"
                    },
                    "adjusted-time": {
                        "color": "$magenta"
                    },
                    "skewed-time": {
                        "color": "$yellow"
                    },
                    "offset-time": {
                        "color": "$cyan"
                    },
                    "file-offset": {
                        "color": "#4c566a"
                    },
                    "invalid-msg": {
                        "color": "$yellow"
                    },
                    "focused": {
                        "color": "$black",
                        "background-color": "$white"
                    },
                    "disabled-focused": {
                        "color": "$white",
                        "background-color": "#3b4252"
                    },
                    "popup": {
                        "color": "$black",
                        "background-color": "$cyan"
                    },
                    "scrollbar": {
                        "color": "$black",
                        "background-color": "#4c566a"
                    },
                    "h1": {
                        "color": "$purple",
                        "bold": true
                    },
                    "h2": {
                        "color": "$purple",
                        "underline": true
                    },
                    "h3": {
                        "color": "$purple"
                    },
                    "h4": {
                        "underline": true
                    },
                    "h5": {
                        "underline": true
                    },
                    "h6": {
                        "underline": true
                    },
                    "hr": {
                        "color": "#81a1c1"
                    },
                    "hyperlink": {
                        "underline": true
                    },
                    "list-glyph": {
                        "color": "$cyan"
                    },
                    "breadcrumb": {
                        "color": "#434c5e",
                        "bold": true
                    },
                    "table-border": {
                        "color": "#434c5e"
                    },
                    "table-header": {
                        "bold": true
                    },
                    "quote-border": {
                        "color": "$white",
                        "background-color": "#3b4252"
                    },
                    "quoted-text": {
                        "color": "$yellow",
                        "background-color": "#4c566a"
                    },
                    "footnote-border": {
                        "color": "$blue",
                        "background-color": "#4c566a"
                    },
                    "footnote-text": {
                        "color": "$cyan",
                        "background-color": "#4c566a"
                    },
                    "snippet-border": {
                        "color": "$cyan"
                    },
                    "indent-guide": {
                        "color": "#4c566a"
                    }
                },
                "syntax-styles": {
                    "inline-code": {
                        "color": "$green",
                        "background-color": "#2e3440"
                    },
                    "quoted-code": {
                        "color": "$orange",
                        "background-color": "#2e3440"
                    },
                    "code-border": {
                        "color": "$white",
                        "background-color": "#2e3440"
                    },
                    "keyword": {
                        "color": "$pink",
                        "bold": true
                    },
                    "string": {
                        "color": "$yellow",
                        "bold": true
                    },
                    "comment": {
                        "color": "#6272A4"
                    },
                    "doc-directive": {
                        "color": "$pink"
                    },
                    "variable": {
                        "color": "$orange"
                    },
                    "symbol": {
                        "color": "$cyan"
                    },
                    "re-special": {
                        "color": "$cyan"
                    },
                    "re-repeat": {
                        "color": "$yellow"
                    },
                    "diff-delete": {
                        "color": "$red"
                    },
                    "diff-add": {
                        "color": "$green"
                    },
                    "diff-section": {
                        "color": "#81a1c1"
                    },
                    "spectrogram-low": {
                        "background-color": "$green"
                    },
                    "spectrogram-medium": {
                        "background-color": "$yellow"
                    },
                    "spectrogram-high": {
                        "background-color": "$red"
                    },
                    "file": {
                        "color": "$blue"
                    },
                    "null": {
                        "color": "#4c566a"
                    },
                    "ascii-control": {
                        "color": "$green"
                    },
                    "non-ascii": {
                        "color": "$yellow"
                    },
                    "number": {
                        "bold": true
                    },
                    "function": {
                        "color": "$green"
                    },
                    "separators-references-accessors": {
                        "color": "$pink"
                    },
                    "type": {
                        "color": "$cyan"
                    }
                },
                "status-styles": {
                    "disabled-title": {
                        "color": "$blue",
                        "background-color": "#3b4252",
                        "bold": true
                    },
                    "title": {
                        "color": "$white",
                        "background-color": "$blue",
                        "bold": true
                    },
                    "subtitle": {
                        "color": "#4c566a",
                        "background-color": "#88c0d0",
                        "bold": true
                    },
                    "info": {
                        "color": "#d8dee9",
                        "background-color": "#2e3440"
                    },
                    "title-hotkey": {
                        "color": "$black",
                        "background-color": "$blue",
                        "underline": true
                    },
                    "hotkey": {
                        "color": "$white",
                        "underline": true
                    },
                    "text": {
                        "color": "$white",
                        "background-color": "#3b4252"
                    },
                    "warn": {
                        "color": "$yellow",
                        "background-color": "#3b4252"
                    },
                    "alert": {
                        "color": "$red",
                        "background-color": "#3b4252"
                    },
                    "active": {
                        "color": "$green",
                        "background-color": "#3b4252"
                    },
                    "inactive": {
                        "color": "#d8dee9",
                        "background-color": "#2e3440"
                    },
                    "inactive-alert": {
                        "color": "$red",
                        "background-color": "#2e3440"
                    },
                    "suggestion": {
                        "color": "#4c566a"
                    }
                },
                "log-level-styles": {
                    "warning": {
                        "color": "$yellow"
                    },
                    "error": {
                        "color": "$red"
                    },
                    "critical": {
                        "color": "$red"
                    },
                    "fatal": {
                        "color": "$red"
                    }
                }
            }
        }
    }
}
